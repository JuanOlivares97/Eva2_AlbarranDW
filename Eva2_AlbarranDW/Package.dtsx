                  UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor">SET LANGUAGE 'Spanish';
SELECT DISTINCT
    CONVERT(NVARCHAR(8), fecha, 112) AS IDFecha,
	fecha AS DescFecha,
    YEAR(fecha) AS DescAnio,
    MONTH(fecha) AS DescMes,
    DATENAME(month, fecha) AS DescNombreMes,
    DAY(fecha) AS DescDia,
