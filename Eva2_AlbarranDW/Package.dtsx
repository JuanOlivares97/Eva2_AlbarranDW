        WHEN MONTH(fecha) IN (4, 5, 6) THEN 2
        WHEN MONTH(fecha) IN (7, 8, 9) THEN 3
        WHEN MONTH(fecha) IN (10, 11, 12) THEN 4
    END AS DescTrimestre,
    DATEPART(week, fecha) AS DescSemanaAnio
FROM Albarran.dbo.ventas
